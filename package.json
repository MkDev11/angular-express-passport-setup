{
  "name": "ng4-express-passport-setup",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng-start": "cd ./public/passport-app && tsc && npm start",
    "express-start": "tsc-watch --onSuccess 'nodemon ./bin/www'",
    "sq:migrate": "sequelize db:migrate",
    "sq:migrate:undo": "sequelize db:migrate:undo",
    "sq:migrate:undo:all": "sequelize db:migrate:undo:all",
    "start": "concurrently \"npm run express-start\"  \"npm run ng-start\""
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "express-session": "^1.15.6",
    "morgan": "~1.8.1",
    "mysql": "^2.15.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.37.7",
    "sequelize-cli": "^4.0.0",
    "sequelize-typescript": "^1.0.0-alpha.3",
    "serve-favicon": "~2.4.2",
    "ts-node": "^6.0.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@babel/cli": "^7.0.0-beta.45",
    "@babel/core": "^7.0.0-beta.45",
    "@babel/node": "^7.0.0-beta.45",
    "@babel/preset-env": "^7.0.0-beta.45",
    "@babel/preset-typescript": "^7.0.0-beta.45",
    "@babel/register": "^7.0.0-beta.45",
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jasmine": "^2.8.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "@types/passport": "^0.4.5",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/passport-twitter": "^1.0.34",
    "@types/sequelize": "^4.27.18",
    "concurrently": "^3.5.0",
    "dotenv": "^5.0.1",
    "nodemon": "^1.12.1",
    "snakecase-keys": "^1.1.1",
    "tsc-watch": "^1.0.21",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "typescript-register": "^1.1.0"
  }
}
